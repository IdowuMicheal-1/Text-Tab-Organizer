{
    "manifest_version": 3,
    "version":"1.0.0",
    "description":"save tabs easily and apply any note to the link",
    "name":"Tab",
    "permissions":["tabs","storage"],
    "actions":{
        "default_popup":"popup.html"
    },
    "content_script":[
        {
            "matches":["<all_urls>"],
            "js":["contentscript.js"],
            "css"
        }
    ]
}